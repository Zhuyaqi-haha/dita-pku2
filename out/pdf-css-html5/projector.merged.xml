                <colspec class="- topic/colspec " colname="col1" colnum="1" colwidth="1*" xtrc="colspec:1;9:65" xtrf="file:/D:/桌面/dita-pku%202/reference.dita" style="width:50%;"/>
                <colspec class="- topic/colspec " colname="col2" colnum="2" colwidth="1*" xtrc="colspec:2;10:65" xtrf="file:/D:/桌面/dita-pku%202/reference.dita" style="width:50%;"/>
